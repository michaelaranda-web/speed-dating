<h1>HELLO THIS IS SPEED DATING</h1>

<div>
    <%= form_tag({:action => :upload}, :multipart => true) do %>
      <label for="num_tables">Number of tables:</label>
      <input id="num_tables" name="num_tables" type="text" />
      <br />
      <label for="num_rounds">Number of rounds:</label>
      <input id="num_rounds" name="num_rounds" type="text" />
      <br />
      <label for="attendees">Upload CSV file with list of attendees (name,age,gender,phone number):</label>
      <br />
      <%= file_field_tag 'attendees' %>
      <br />
      <div class="actions">
        <%= submit_tag 'Submit' %>
      </div>
    <% end %>
</div>